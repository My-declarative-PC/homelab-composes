# ID пользователя на хосте (проверьте командой `id`)
PUID=1000

# ID группы на хосте (проверьте командой `id`)
PGID=1000

# Ваша часовая зона
TZ=Europe/Moscow

# Ваш поддомен в DuckDNS (например, example)
SUBDOMAINS=your-subdomain

# Ваш токен DuckDNS
TOKEN=your-duckdns-token

# (Опционально) Включить логирование в файл
LOG_FILE=true
